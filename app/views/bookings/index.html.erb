<h1>My dashboard:</h1>

<h3>My boats to rent:</h3>
<ul>
  <% current_user.boats.each do |boat| %>
  <li>
    <%= link_to boat.name, boat_path(boat)%>
    <%= link_to "Edit", edit_boat_path(boat)%>
    <%= link_to "Delete", boat_path(boat), data: {turbo_method: :delete, turbo_confirm: "Are you sure ?"}%>
  </li>
  <% end  %>
</ul>
